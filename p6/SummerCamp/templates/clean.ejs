<!doctype html>
<html lang="en">

<head>
	<meta charset="utf-8" />
	<title>Admin GFA</title>
</head>

<body>
	<h1>Removing All Applicants</h1>
	<form action="/clean" method="post" id="form">
		<input type="submit" value="Removing all applicants">
	</form>
	<script>
		document.getElementById('form').addEventListener('submit', (e) => checker(e))

		function checker(e) {
			if(!window.confirm("Are you sure you want to submit your application?\nYou will not be able to change your information once submitted"))
				e.preventDefault()
		}
	</script>
    <hr color="gray">
    <a href="/">HOME</a>
</body>

</html>
